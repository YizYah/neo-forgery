{
  "name": "neo-forgery",
  "description": "the easy way to mock a neo4j-driver session",
  "author": "YizYah",
  "version": "1.5.5",
  "bugs": "https://github.com/YizYah/neo-forgery/issues",
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "lib/**/*"
  ],
  "homepage": "https://github.com/YizYah/neo-forgery",
  "keywords": [
    "neo4j",
    "mock",
    "neo4j-driver",
    "test",
    "ts-packrat",
    "geenee"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "YizYah/neo-forgery",
  "scripts": {
    "build": "tsc",
    "commit": "git-cz",
    "lint": "eslint \"src/**/*.ts\"",
    "lintfix": "eslint \"src/**/*.ts\" --fix",
    "test": "ava",
    "posttest": "npm run view-coverage && npm run report && npm run lint",
    "prepack": "rm -rf lib && tsc -b",
    "report": "nyc report --reporter=json",
    "semantic-release": "semantic-release",
    "view-coverage": "nyc --extension .ts ava --forbid-only"
  },
  "dependencies": {
    "@types/node": "^14.14.19",
    "deep-equal": "^2.0.5",
    "neo4j-driver": "^4.2.3",
    "path": "^0.12.7",
    "tslib": "^2.0.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "ava": "^3.15.0",
    "commitizen": "^4.2.3",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "^7.23.0",
    "ghooks": "^2.0.4",
    "nyc": "^15.1.0",
    "prettier": "^2.2.1",
    "proxyquire": "^2.1.3",
    "semantic-release": "^17.4.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
  },
  "ava": {
    "files": [
      "test/**/*.test.ts"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test"
    }
  }
}
